{
    "name": "dev-server-ports",
    "description": "Gracefully pick a port number for the dev server",
    "version": "0.1.0",
    "license": "MIT",
    "author": "Brion Mario",
    "keywords": [
        "dev-server-ports",
        "dynamic-port",
        "webpack-dynamic-port",
        "port-finder"
    ],
    "homepage": "https://github.com/brionmario/dev-server-ports#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/brionmario/dev-server-ports"
    },
    "bugs": {
        "url": "https://github.com/brionmario/dev-server-ports/issues"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "analyze": "size-limit --why",
        "build": "tsdx build",
        "cm": "cz",
        "lint": "eslint --ext .js,.ts .",
        "prepare": "tsdx build",
        "size": "size-limit",
        "start": "tsdx watch",
        "test": "tsdx test"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
        }
    },
    "module": "dist/dynamic-dev-server-port.esm.js",
    "size-limit": [
        {
            "path": "dist/dynamic-dev-server-port.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/dynamic-dev-server-port.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "@types/detect-port": "^1.3.2",
        "@types/inquirer": "^8.2.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.12.0",
        "eslint-plugin-import": "^2.25.4",
        "husky": "^7.0.4",
        "size-limit": "^7.0.8",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "chalk": "^4.1.2",
        "detect-port": "^1.3.0",
        "inquirer": "^8.2.2",
        "is-root": "^2.1.0"
    }
}
